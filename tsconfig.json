{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "./dist/",
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
    }
  },
}